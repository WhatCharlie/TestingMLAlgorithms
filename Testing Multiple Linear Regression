    "def create_naive_Linear_Regression_2D_Graph(intercept, slope1, slope2, dataset):\n",
    "    color = 'blue'\n",
    "    color2 = 'green'\n",
    "    linecolor = \"lightblue\"\n",
    "    linecolor2 = \"lightgreen\"\n",
    "    \n",
    "    x1_Linear_Regression = dataset[Column1]\n",
    "    x2_Linear_Regression = dataset[Column2]\n",
    "    y_Linear_Regression = intercept + (slope1 * x1_Linear_Regression) + (slope2 * x2_Linear_Regression)\n",
    "    x_Linear_Regression = ((dataset[Column1] + dataset[Column2]) / 2)\n",
    "    \n",
    "    plt.scatter(x_Linear_Regression, y_Linear_Regression, s=size, zorder=3, facecolors='none', edgecolors='red')\n",
    "\n",
    "create_Original_Graph(rd)\n",
    "create_naive_Linear_Regression_2D_Graph(B0, B1, B2, rd)\n",
    "plt.show()\n",
    "\n",
    "def create_Linear_Regression_Plane_Multiple_Views(intercept, slope1, slope2, dataset, column1, column2, column3):\n",
    "    x1_Linear_Regression = dataset[column1]\n",
    "    x2_Linear_Regression = dataset[column2]\n",
    "    y_actual = dataset[column3]\n",
    "    \n",
    "    y_Linear_Regression = intercept + (slope1 * x1_Linear_Regression) + (slope2 * x2_Linear_Regression)\n",
    "    \n",
    "    # Create a 2x2 grid of subplots\n",
    "    fig = plt.figure(figsize=(20, 16))\n",
    "    \n",
    "    # List of viewing angles (elevation, azimuth)\n",
    "    views = [(0, 45), (0, 135), (0, 225), (0, 315)]\n",
    "    \n",
    "    for i, (elev, azim) in enumerate(views, 1):\n",
    "        ax = fig.add_subplot(2, 2, i, projection='3d')\n",
    "        \n",
    "        # Plot the actual data points\n",
    "        ax.scatter(x1_Linear_Regression, x2_Linear_Regression, y_actual, c='teal', marker='o', label='Actual Data', s=(size*2))\n",
    "        \n",
    "        # Create a mesh grid for the regression plane\n",
    "        x1_surf, x2_surf = np.meshgrid(np.linspace(x1_Linear_Regression.min(), x1_Linear_Regression.max(), 100),\n",
    "                                       np.linspace(x2_Linear_Regression.min(), x2_Linear_Regression.max(), 100))\n",
    "        y_surf = intercept + (slope1 * x1_surf) + (slope2 * x2_surf)\n",
    "        \n",
    "        # Plot the regression plane\n",
    "        ax.plot_surface(x1_surf, x2_surf, y_surf, alpha=0.4, cmap='inferno', label='Regression Plane')\n",
    "        \n",
    "        ax.set_xlabel(column1)\n",
    "        ax.set_ylabel(column2)\n",
    "        ax.set_zlabel(column3)\n",
    "        ax.legend()\n",
    "        \n",
    "        # Set the viewing angle\n",
    "        ax.view_init(elev=elev, azim=azim)\n",
    "        ax.set_title(f'View from {azim}°')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    return fig\n",
    "\n",
    "# Use the function\n",
    "fig = create_Linear_Regression_Plane_Multiple_Views(B0, B1, B2, rd, Column1, Column2, Column3)\n",
    "plt.show()"
   ]
