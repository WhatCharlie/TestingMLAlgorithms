    "def find_slopes(x_mean1, x_mean2, y_mean, samples, dataset):\n",
    "    n = samples\n",
    "    sum_x2_sq, sum_x1y, sum_x1x2, sum_x2y, sum_x1_sq, sq_sum_x1x2 = 0, 0, 0, 0, 0, 0\n",
    "\n",
    "    for row in range(n):\n",
    "        \n",
    "        x1i = dataset[Column1][row]\n",
    "        x2i = dataset[Column2][row]\n",
    "        yi = dataset[Column3][row]\n",
    "\n",
    "        sum_x2_sq += ((x2i - x_mean2) ** 2)\n",
    "        sum_x1y += ((x1i - x_mean1) * (yi - y_mean))\n",
    "        sum_x1x2 += ((x1i - x_mean1) * (x2i - x_mean2))\n",
    "        sum_x2y += ((x2i - x_mean2) * (yi - y_mean))\n",
    "\n",
    "        sum_x1_sq += ((x1i - x_mean1) ** 2)\n",
    "\n",
    "    sq_sum_x1x2 = sum_x1x2 ** 2\n",
    "\n",
    "    denominator = (sum_x1_sq * sum_x2_sq) - sq_sum_x1x2\n",
    "    B1 = ((sum_x2_sq * sum_x1y) - (sum_x1x2 * sum_x2y)) / denominator\n",
    "    B2 = ((sum_x1_sq * sum_x2y) - (sum_x1x2 * sum_x1y)) / denominator\n",
    "\n",
    "\n",
    "    return B1, B2\n",
    "\n",
    "B1, B2 = find_slopes(x1_Sample_Mean, x2_Sample_Mean, y_Sample_Mean, num_rows, rd)\n",
    "\n",
    "print(f\"'B1', or the Slope of the 1st Predictor: {B1}\\n'B2', or the Slope of the 2nd Predictor: {B2}\")"
   ]
  },
