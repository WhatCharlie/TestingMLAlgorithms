    "def find_Residual_Sum_of_Squares(intercept, slope1, slope2, samples, dataset):\n",
    "    \n",
    "    RSS = 0\n",
    "    squared_errors = []\n",
    "    \n",
    "    for row in range(samples):\n",
    "        \n",
    "        x1i = dataset[Column1][row]\n",
    "        x2i = dataset[Column2][row]\n",
    "        yi = dataset[Column3][row]\n",
    "        \n",
    "        squared_error = (yi - (intercept + ((slope1 * x1i) + (slope2 * x2i)))) ** 2\n",
    "\n",
    "        RSS += squared_error\n",
    "        squared_errors.append(squared_error)\n",
    "        MSE = RSS / samples\n",
    "    return RSS, MSE, squared_errors\n",
    "\n",
    "RSS_Result = find_Residual_Sum_of_Squares(B0, B1, B2, num_rows, rd)\n",
    "RSS, MSE, squared_errors = RSS_Result\n",
    "\n",
    "print(f\"Residiual Sum of Squares: {RSS} \\n\\nMSE: {MSE} \\n\\nList of Squared Errors: {squared_errors}\")"
   ]
