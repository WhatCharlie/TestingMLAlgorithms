   "id": "59a2696c-b5c7-4d1e-8bdd-c5e686346f11",
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_Total_Sum_of_Squares(y_mean, samples, dataset):\n",
    "    \n",
    "    TSS = 0\n",
    "    \n",
    "    for row in range(samples):\n",
    "        yi = dataset[Column3][row]\n",
    "        TSS += (yi - y_mean) ** 2\n",
    "        \n",
    "    return TSS\n",
    "    \n",
    "TSS = find_Total_Sum_of_Squares(y_Sample_Mean, num_rows, rd)\n",
    "print(f\"The Total Sum of Squares is: {TSS}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ede0d666-c983-4a7c-9137-5c5ed1fa08eb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_F_Stat(tss, rss, predictors, samples):\n",
    "    n = samples\n",
    "    p = predictors\n",
    "    \n",
    "    numerator = (tss - rss) / p\n",
    "    denominator = rss / (n - p - 1)\n",
    "    \n",
    "    F_stat = numerator / denominator\n",
    "    \n",
    "    return F_stat\n",
    "    \n",
    "F_stat = find_F_Stat(TSS, RSS, 2, num_rows)\n",
    "print(f\"The F-statistic is: {F_stat}\")"
   ]
  }
