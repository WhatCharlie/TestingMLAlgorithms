   "source": [
    "def find_intercept(x1_mean, x2_mean, y_mean, slope1, slope2):\n",
    "    \n",
    "    B0 = y_mean - (slope1 * x1_mean) - (slope2 * x2_mean)\n",
    "\n",
    "    return B0\n",
    "\n",
    "B0 = find_intercept(x1_Sample_Mean, x2_Sample_Mean, y_Sample_Mean, B1, B2)\n",
    "\n",
    "print(f\"'B0', or the Y-Intercept: {B0}\")"
   ]
