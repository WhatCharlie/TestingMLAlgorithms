    "def find_Sample_Means(samples, dataset):\n",
    "    n = samples\n",
    "    \n",
    "    x1_Sample_Sum = 0\n",
    "    x2_Sample_Sum = 0\n",
    "    y_Sample_Sum = 0\n",
    "    \n",
    "    for row in range(n):\n",
    "        y_Sample_Sum += dataset[Column3][row]\n",
    "        x1_Sample_Sum += dataset[Column1][row]\n",
    "        x2_Sample_Sum += dataset[Column2][row]\n",
    "    \n",
    "    y_Sample_Mean = y_Sample_Sum / n\n",
    "    x1_Sample_Mean = x1_Sample_Sum / n\n",
    "    x2_Sample_Mean = x2_Sample_Sum / n\n",
    "    return x1_Sample_Mean, x2_Sample_Mean, y_Sample_Mean\n",
    "\n",
    "sample_Means = find_Sample_Means(num_rows, rd)\n",
    "x1_Sample_Mean, x2_Sample_Mean, y_Sample_Mean = sample_Means\n",
    "print(f\"X1 samples Mean: {x1_Sample_Mean} \\nX2 samples Mean: {x2_Sample_Mean}\\nY samples Mean: {y_Sample_Mean}\")"
   ]
