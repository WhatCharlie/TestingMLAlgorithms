    "def create_2D_slices(intercept, slope1, slope2, dataset, column1, column2, column3):\n",
    "    fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(15, 6))\n",
    "    \n",
    "    # Y vs X1 for median X2\n",
    "    median_x2 = dataset[column2].median()\n",
    "    ax1.scatter(dataset[column1], dataset[column3], alpha=0.5, color='blue', s=size)\n",
    "    x1_range = np.linspace(dataset[column1].min(), dataset[column1].max(), 100)\n",
    "    y_pred = intercept + slope1 * x1_range + slope2 * median_x2\n",
    "    ax1.plot(x1_range, y_pred, color='red')\n",
    "    ax1.set_xlabel(column1)\n",
    "    ax1.set_ylabel(column3)\n",
    "    ax1.set_title(f'{column3} vs {column1} (at median {column2})')\n",
    "    \n",
    "    # Y vs X2 for median X1\n",
    "    median_x1 = dataset[column1].median()\n",
    "    ax2.scatter(dataset[column2], dataset[column3], alpha=0.5, color='green', s=size)\n",
    "    x2_range = np.linspace(dataset[column2].min(), dataset[column2].max(), 100)\n",
    "    y_pred = intercept + slope1 * median_x1 + slope2 * x2_range\n",
    "    ax2.plot(x2_range, y_pred, color='red')\n",
    "    ax2.set_xlabel(column2)\n",
    "    ax2.set_ylabel(column3)\n",
    "    ax2.set_title(f'{column3} vs {column2} (at median {column1})')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "def create_residual_plot(intercept, slope1, slope2, dataset, column1, column2, column3):\n",
    "    y_pred = intercept + slope1 * dataset[column1] + slope2 * dataset[column2]\n",
    "    residuals = dataset[column3] - y_pred\n",
    "    \n",
    "    fig, ax = plt.subplots(figsize=(10, 6))\n",
    "    ax.scatter(y_pred, residuals, alpha=0.5, color='teal', s=size)\n",
    "    ax.axhline(y=0, color='r', linestyle='--')\n",
    "    ax.set_xlabel('Predicted Values')\n",
    "    ax.set_ylabel('Residuals')\n",
    "    ax.set_title('Residual Plot')\n",
    "    plt.show()\n",
    "\n",
    "def create_contour_plot(intercept, slope1, slope2, dataset, column1, column2, column3):\n",
    "    x1_range = np.linspace(dataset[column1].min(), dataset[column1].max(), 100)\n",
    "    x2_range = np.linspace(dataset[column2].min(), dataset[column2].max(), 100)\n",
    "    X1, X2 = np.meshgrid(x1_range, x2_range)\n",
    "    Y = intercept + slope1 * X1 + slope2 * X2\n",
    "    \n",
    "    fig, ax = plt.subplots(figsize=(10, 8))\n",
    "    contour = ax.contourf(X1, X2, Y, levels=20, cmap='inferno')\n",
    "    ax.scatter(dataset[column1], dataset[column2], c=dataset[column3], cmap='inferno', linewidths=0.5, edgecolor='black', s=size+10)\n",
    "    ax.set_xlabel(column1)\n",
    "    ax.set_ylabel(column2)\n",
    "    ax.set_title(f'Contour Plot of {column3}')\n",
    "    fig.colorbar(contour, label=column3)\n",
    "    plt.show()\n",
    "\n",
    "# Use these functions\n",
    "create_2D_slices(B0, B1, B2, rd, Column1, Column2, Column3)\n",
    "create_residual_plot(B0, B1, B2, rd, Column1, Column2, Column3)\n",
    "create_contour_plot(B0, B1, B2, rd, Column1, Column2, Column3)"
   ]
  },
