    "def init_slope_and_intercept(dataset):\n",
    "    B0 = 0\n",
    "    B1 = 0\n",
    "    B = 0.0000001\n",
    "    B0_list = []\n",
    "    B1_list = []\n",
    "    for row in range(num_rows):\n",
    "        B0_list.append(0)\n",
    "        B1_list.append(0)\n",
    "    return B0, B1, B0_list, B1_list, B\n",
    "\n",
    "B0, B1, B0_list, B1_list, B = init_slope_and_intercept(rd)\n",
    "print(B0, B1, B0_list, B1_list)"
   ]
